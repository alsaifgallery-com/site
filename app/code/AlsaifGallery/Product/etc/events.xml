<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <!--for cart loaded-->
    <event name="sales_quote_load_after">
        <observer name="alsaifgallery_product_sales_quote_load_after" instance="AlsaifGallery\Product\Observer\ProductInterface" />
    </event>
    
    <!--for the item added for cart-->
    <event name="sales_quote_save_after">
        <observer name="alsaifgallery_product_sales_quote_save_after" instance="AlsaifGallery\Product\Observer\ProductInterface" />
    </event>

</config>